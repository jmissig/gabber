#: gabber.schemas.in.h:5 ui/Prefs_win.glade.h:14
msgid "Automatically display messages when Available or Free for Chat"
msgstr "Mostrar mesnagens automaticamente quando Disponível ou Livre para Conversa"

#: gabber.schemas.in.h:6
msgid "Automatically reconnect when disconnected"
msgstr "Reconectar automaticamente quando desconectado"

#: gabber.schemas.in.h:7
msgid "Away after x minutes"
msgstr "Ausente depois de x minutos"

#: gabber.schemas.in.h:8 ui/Prefs_win.glade.h:17
msgid "Check messages for spelling mistakes"
msgstr "Verificar ortografia das mensagens"

#: gabber.schemas.in.h:9
msgid "Extended Away after x minutes"
msgstr "Ausência extendida depois de x minutos"

#: gabber.schemas.in.h:10
msgid "Groups that are expanded between sessions."
msgstr "Grupos que são expandidos entre sessões."

#: gabber.schemas.in.h:11
msgid "Hide Offline Contacts"
